{
  "name": "mockee",
  "version": "0.2.4",
  "description": "App for mocking API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "npm-run-all --parallel start-electron dev-main dev-renderer",
    "build": "npm run build-main & npm run build-renderer",
    "start-electron": "electron .",
    "dev-renderer": "webpack --watch",
    "build-renderer": "webpack",
    "dev-main": "npm-watch",
    "build-main": "babel src --out-dir dist --ignore renderer --source-maps",
    "lint": "eslint 'src/**/*.js'",
    "test": "jest",
    "tdd": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "watch": {
    "build-main": {
      "patterns": [
        "src/main/**/*.js",
        "!src/**/*.test.js"
      ]
    }
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "mock api"
  ],
  "author": "clixr",
  "license": "CC0-1.0",
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-istanbul": "^2.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "css-loader": "^0.26.1",
    "electron": "^1.4.12",
    "electron-devtools-installer": "^2.0.1",
    "electron-prebuilt": "^1.4.12",
    "electron-reload": "^1.0.3",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "file-loader": "^0.10.1",
    "jest": "^17.0.3",
    "node-sass": "^4.0.0",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^1.2.1",
    "react-addons-test-utils": "^15.4.2",
    "redux-mock-store": "^1.2.1",
    "sass-loader": "^4.1.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3"
  },
  "dependencies": {
    "atob": "^2.0.3",
    "btoa": "^1.1.2",
    "classnames": "^2.2.5",
    "deep-equal": "^1.0.1",
    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.4.5",
    "express": "^4.14.0",
    "font-awesome": "^4.7.0",
    "fun-task": "^1.5.2",
    "immutable": "^3.8.1",
    "json8": "^0.9.2",
    "node-unique": "^1.0.0",
    "npm-watch": "^0.1.8",
    "ramda": "^0.23.0",
    "ramda-fantasy": "^0.7.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-view-manager": "^0.1.0",
    "redux": "^3.6.0",
    "redux-immutable": "^3.0.8",
    "redux-observable": "^0.13.0",
    "reselect": "^2.5.4",
    "rxjs": "5",
    "source-map-support": "^0.4.11",
    "ws": "^1.1.1"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ],
    "moduleNameMapper": {
      "\\.(png|jpg|svg)$": "<rootDir>/jest/webpackAssetsMock.js",
      "\\.(css|scss)$": "<rootDir>/jest/stylesMock.js"
    },
    "testPathDirs": [
      "<rootDir>/src"
    ]
  }
}
