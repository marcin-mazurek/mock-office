{
  "name": "chan",
  "version": "0.7.0",
  "description": "App for mocking API",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build-gui": "webpack --progress",
    "watch-gui": "npm run watch-webpack",
    "watch-webpack": "webpack-dev-server --config webpack-dev.config.js",
    "browser-sync": "browser-sync start --no-ghost-mode --files 'dist/lib/gui/**/*' --proxy http://localhost:3070 --reload-debounce 1000",
    "test-gui": "jest src/gui",
    "tdd-gui": "jest src/gui --watch",
    "build-app": "babel src --out-dir dist --source-maps --ignore src/lib/gui/",
    "watch-app": "babel src --out-dir dist --source-maps --watch --ignore src/lib/gui/",
    "watch-all": "npm-run-all --parallel watch-gui watch-app",
    "watch": "npm-run-all clean watch-all",
    "build": "npm-run-all clean build-gui build-app",
    "dev": "npm-run-all build watch",
    "pm2-run": "pm2 start ecosystem.config.js --only app-server",
    "pm2-run-with-gui": "pm2 start ecosystem.config.js --only app-server-with-gui",
    "pm2-log": "tail -F pm2.log",
    "pm2-log-errors": "tail -F pm2-errors.log",
    "start": "node ./dist/bin/chan.js",
    "start-with-gui": "node ./dist/bin/chan.js --gui",
    "run": "npm-run-all build start",
    "run-with-gui": "npm-run-all build start-with-gui",
    "lint": "eslint 'src/**/*.js'",
    "test": "jest --runInBand",
    "tdd": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "watch": {
    "build-main": {
      "patterns": [
        "src/main/**/*.js",
        "!src/**/*.test.js"
      ]
    }
  },
  "repository": "https://bitbucket.org/clixr/chan",
  "keywords": [
    "mock api"
  ],
  "author": "clixr",
  "license": "CC0-1.0",
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-istanbul": "^2.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.11.1",
    "browser-sync": "^2.18.13",
    "css-loader": "^0.26.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "jest-fetch-mock": "^1.1.1",
    "node-sass": "^4.0.0",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^1.2.1",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.5.4",
    "redux-mock-store": "^1.2.1",
    "request": "^2.81.0",
    "sass-loader": "^4.1.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "ajv": "^4.11.6",
    "atob": "^2.0.3",
    "body-parser": "^1.17.1",
    "btoa": "^1.1.2",
    "classnames": "^2.2.5",
    "colors": "^1.1.2",
    "command-line-args": "^4.0.2",
    "cors": "^2.8.3",
    "cuid": "^1.3.8",
    "deep-equal": "^1.0.1",
    "enzyme-to-json": "^1.4.5",
    "express": "^4.16.2",
    "font-awesome": "^4.7.0",
    "fun-task": "^1.5.2",
    "immutable": "^3.8.1",
    "json8": "^0.9.2",
    "modalo": "^0.1.8",
    "notifications": "git@github.com:xclix/notifications.git",
    "npm-watch": "^0.1.8",
    "prop-types": "^15.5.7",
    "ramda": "^0.23.0",
    "ramda-fantasy": "^0.7.0",
    "rc-switch": "^1.4.4",
    "react": "^15.4.1",
    "react-custom-scrollbars": "^4.0.2",
    "react-dom": "^15.4.1",
    "react-immutable-proptypes": "^2.1.0",
    "react-motion": "^0.5.1",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-select": "^1.0.0-rc.4",
    "redux": "^3.6.0",
    "redux-form": "^6.6.3",
    "redux-immutable": "^3.0.8",
    "redux-observable": "^0.13.0",
    "reselect": "^2.5.4",
    "rxjs": "5",
    "source-map-support": "^0.4.11",
    "whatwg-fetch": "^2.0.3",
    "ws": "^1.1.1"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ],
    "moduleNameMapper": {
      "\\.(png|jpg|svg)$": "<rootDir>/jest/webpackAssetsMock.js",
      "\\.(css|scss)$": "<rootDir>/jest/stylesMock.js"
    },
    "setupFiles": [
      "./jest/fetchMock.js",
      "./jest/rxSetup.js"
    ],
    "roots": [
      "<rootDir>/src",
      "<rootDir>/test"
    ]
  },
  "bin": {
    "chan": "./dist/bin/chan.js"
  }
}
